@model Empleado


@{
	ViewData["Title"] = "Guardar Empleado";
     Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Index</title>
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

</head>


<body>

<h1>Guardar empleado </h1>

<form asp-action="GuardarEmpleado" asp-controller="Empleado" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">Nombre</label>
        <input asp-for="emple_nombre" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Apellido</label>
        <input asp-for="emple_apellido" type="text" class="form-control">
    </div>
      <div class="form-group">
        <label for="exampleInputPassword1">Id Supervisor</label>
        <input asp-for="emple_id_supervisor" type="text" class="form-control">
    </div>
  <div class="form-group">
        <label for="exampleInputPassword1">Id usuario</label>
        <input asp-for="emple_id_usuario" type="text" class="form-control">
    </div>   
 

    <button name="submit" type="submit" id="guardar-submit" data-submit="...Sending">Submit</button>
  
</form>
</body>
</html>


